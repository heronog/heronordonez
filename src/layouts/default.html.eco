<!DOCTYPE html>
<html>
  <head>
    <title><%= @getPreparedTitle() %></title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="/css/bootstrap-journal.css" rel="stylesheet" media="screen">
    <link rel="stylesheet" href="/css/bootstrap-responsive.css" type="text/css" media="screen" title="no title" charset="utf-8">
    <link rel="stylesheet" href="/vendor/fontawesome/css/font-awesome.css" type="text/css" media="screen" title="no title" charset="utf-8">
    <!--[if IE 7]>
      <link rel="stylesheet" href="/vendor/fontawesome/css/font-awesome-ie7.min.css" type="text/css" media="screen" title="no title" charset="utf-8">
    <![endif]-->
    <link rel="stylesheet" href="/css/style.css" type="text/css" media="screen" title="no title" charset="utf-8">
  </head>
  <body>
    <div id="container">
      <header id="nav_bar" class="container">
        <div class="navbar navbar-fixed-top navbar-inverse">
          <div class="navbar-inner">
            <div class="container">
    					<!-- .btn-navbar is used as the toggle for collapsed navbar content -->
    					<a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
    						<span class="icon-bar"></span>
    						<span class="icon-bar"></span>
    						<span class="icon-bar"></span>
    					</a>
              
              <a class="brand" title="Home" href="/">
                <i class="icon-home"></i>
              </a>
              
            </div>
          </div>
        </div>
      </header>
      <article id="main-content" class="container">
        <%- @content %>
      </div>
    	<footer>
    		<%= @partial('footer.html') %>
    	</footer>
    </div>
    <script src="http://code.jquery.com/jquery.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="/js/ui.js" type="text/javascript" charset="utf-8"></script>
    
  </body>
</html>
